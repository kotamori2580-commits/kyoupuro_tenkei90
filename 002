/**
 * 難易度（体感）：★★★☆☆
 * 結果： できなかった
 * 解法：深さ優先探索
 * * 【学んだこと・ポイント】
 * ・DFSの書き方を学んだ
 * ・全列挙　→　深さ優先探索/幅優先探索
 * * 【反省・詰まった点】
 * ・
 * * 【計算量】
 * ・計算量は O(N * C_{N/2}) です。
ここで C_k は カタラン数
 */

#include <bits/stdc++.h>
using namespace std;

int N;
void dfs(int l, int r, string current){
  if(current.length() == N){
    cout << current << "\n";
  }
  else{
    if(l<N/2){
      dfs(l+1, r, current + '(');
    }
    if(r<l){
      dfs(l, r+1, current + ')');
    }
  }
}

int main(){
  cin >> N;
  if(N%2 != 0){
    cout << "" << "\n";
  }
  else{
    dfs(0, 0, "");
  }
}
