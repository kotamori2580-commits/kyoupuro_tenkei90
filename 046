/**
 * 難易度（体感）：★★★☆☆
 * 結果：できなかった
 * 解法：余りに着目する
 * * 【学んだこと・ポイント】
 * ・余りに着目する
 * * 【反省・詰まった点】
 * ・
 * * 【計算量】
 * ・O(N+46**3)
 */

#include <bits/stdc++.h>
using namespace std;

long long A[100009];
long long B[100009];
long long C[100009];

int main(){
  long long N;
  cin >> N;
  for(int i=1;i<=N;i++){
    cin >> A[i];
  }
  for(int i=1;i<=N;i++){
    cin >> B[i];
  }
  for(int i=1;i<=N;i++){
    cin >> C[i];
  }
  
  long long countA[46];
  long long countB[46];
  long long countC[46];
  for(int i=1;i<=N;i++){
    int a;
    a = A[i]%46;
    countA[a] ++;
  }
  for(int i=1;i<=N;i++){
    int b;
    b = B[i]%46;
    countB[b] ++;
  }
  for(int i=1;i<=N;i++){
    int c;
    c = C[i]%46;
    countC[c] ++;
  }
  
  long long ans=0;
  for(int i=0;i<=45;i++){
    for(int j=0;j<=45;j++){
      for(int k=0;k<=45;k++){
        if((i+j+k)%46==0){
          ans += countA[i]*countB[j]*countC[k];
        }
      }
    }
  }
  cout << ans << "\n";
}
